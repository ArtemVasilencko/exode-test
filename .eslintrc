{
  "plugins": ["@eslint/js", "typescript-eslint", "react"],
  "extends": [
    "plugin:@eslint/js/recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    },
  },
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
  },
  "rules": {
    "no-unused-vars": "error",
    "no-undef": "error",
  },
}
